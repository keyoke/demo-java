{
    "connectionString": "",
    "roleName": "demo-java",
    "roleInstance": "",
    "telemetryContext": {
        "k8s.pod.name": "demo-java",
        "k8s.pod.namespace": "applications"
    },
    "jmxMetrics": [
        {
            "objectName": "java.lang:type=Threading",
            "attribute": "ThreadCount",
            "display": "Thread Count(JMX)"
        },
        {
            "objectName": "java.lang:type=Memory",
            "attribute": "HeapMemoryUsage.init",
            "display": "Initial Heap Memory(JMX)"
        },
        {
            "objectName": "java.lang:type=ClassLoading",
            "attribute": "LoadedClassCount",
            "display": "Current Loaded Class Count(JMX)"
        }
    ]
}
